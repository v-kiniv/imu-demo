{
  "manifest_version": 2,

  "name": "Mongoose OS :: IMU Visualization",
  "description": "This extension demonstrates Madgwick's IMU filter",
  "version": "1.0",

  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },

  "icons": {
    "32": "img/imu_32.png",
    "128": "img/imu_128.png",
    "1024": "img/imu_1024.png"
  },

  "permissions": [
    "serial",
    "fullscreen", "overrideEscFullscreen",
    "storage"
  ]
}
